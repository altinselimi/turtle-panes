{
  "name": "turtle-panes-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsc --project tsconfig.json && vite",
    "dev:react": "tsc --project tsconfig.react.json && vite --config vite.react.config.ts",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:dev": "vitepress dev docs",
    "merge-dist": "node ./mergeDist.js",
    "netlify-build": "npm run build && npm run docs:build && npm run merge-dist"
  },
  "dependencies": {
    "@turtle-panes/react": "^1.0.4",
    "@turtle-panes/vue": "^1.0.1",
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@icons-pack/react-simple-icons": "^11.0.1",
    "@jest/types": "^29.6.3",
    "@lottiefiles/dotlottie-vue": "^0.5.8",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/vue": "^8.1.0",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitest/coverage-v8": "^2.1.8",
    "@welldone-software/why-did-you-render": "^10.0.1",
    "jsdom": "^25.0.1",
    "lucide-react": "^0.473.0",
    "lucide-vue-next": "^0.408.0",
    "prettier": "^3.3.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0",
    "sass": "^1.83.1",
    "typescript": "^5.2.2",
    "vite": "^5.4.8",
    "vite-plugin-html": "^3.2.2",
    "vitepress": "^1.6.3",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.1.6",
    "vue3-simple-icons": "^13.2.0"
  }
}
